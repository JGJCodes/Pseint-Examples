Proceso Mayor_Igualdad
	
	A<-0;
	B<-0;
	
	Escribir 'Ingresar el primer número';
	Leer A;
	Escribir 'Ingresar el segundo número';
	Leer B;
	
	Si A>B Entonces;
		Escribir 'El mayor es:', A;
	Sino;
		Si A=B Entonces;
			Escribir A, '=', B;
		Sino
			Escribir 'El mayor es:', B;
		FinSi
		
	FinSi
	
FinProceso
